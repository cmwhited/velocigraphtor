{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "pretty": true,
    "alwaysStrict": false,
    "strict": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "typescript-deno-plugin"
      }
    ],
    "target": "esnext"
  }
}
